{
    "name": "KeyMan",
    "version": "1.0.0",
    "manifest_version": 2,
    "description": "account management widget",
    "icons": {
        "48": "icon.png"
    },
    "permissions":[
        "storage",
        "unlimitedStorage"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    }
}